{
  "name": "react-master",
  "version": "1.0.0",
  "description": "Monorepo containing React and Vue projects",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "preview": "turbo run preview",
    "clean": "turbo run clean",
    "install:all": "pnpm install",
    "react:counter": "turbo run dev --filter=react-counter",
    "vue:counter": "turbo run dev --filter=vue-counter",
    "vue:todo": "turbo run dev --filter=vue-todo",
    "vue:todo:electron": "turbo run electron:dev --filter=vue-todo",
    "react:todo": "turbo run dev --filter=react-todo",
    "format": "turbo run format",
    "format:check": "turbo run format:check"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "turbo": "^2.0.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
